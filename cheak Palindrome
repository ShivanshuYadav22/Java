import java.util.*;

public class Palindrome {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter number: ");
        int n = sc.nextInt();

        int temp = n, rev = 0;

        while (temp != 0) {
            int digit = temp % 10;
            rev = rev * 10 + digit;
            temp /= 10;
        }

        if (n == rev)
            System.out.println(n + " is a Palindrome.");
        else
            System.out.println(n + " is Not Palindrome.");
    }
}
